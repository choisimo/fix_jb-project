# =============================================================================
# 데이터베이스 설정
# =============================================================================
DATABASE_USERNAME=jbreport
DATABASE_PASSWORD=your_secure_password_here

# =============================================================================
# Redis 설정
# =============================================================================
REDIS_PASSWORD=your_redis_password_here

# =============================================================================
# JWT 설정
# =============================================================================
JWT_SECRET=your_jwt_secret_key_here

# =============================================================================
# API Base URL
# =============================================================================
API_BASE_URL=http://localhost

# =============================================================================
# Host Upload Path
# =============================================================================
HOST_UPLOAD_PATH=./data/uploads

# =============================================================================
# Roboflow API 설정
# =============================================================================
ROBOFLOW_API_KEY=your_roboflow_api_key_here
ROBOFLOW_WORKSPACE_URL=your_roboflow_workspace_url_here
ROBOFLOW_WORKSPACE=jeonbuk-reports
ROBOFLOW_PROJECT=integrated-detection
ROBOFLOW_VERSION=1
ROBOFLOW_MODEL_ID=your_roboflow_model_id

# =============================================================================
# Google Cloud 설정
# =============================================================================
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_VISION_ENABLED=true
GOOGLE_APPLICATION_CREDENTIALS=/app/google-credentials.json

# =============================================================================
# AI Agent 설정 (OpenRouter)
# =============================================================================
AI_AGENT_ENABLED=true
AI_AGENT_API_URL=https://openrouter.ai/api/v1
AI_AGENT_API_KEY=your_openrouter_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=qwen/qwen2.5-vl-72b-instruct:free

# =============================================================================
# OAuth 설정
# =============================================================================
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_CLIENT_SECRET=your_kakao_client_secret

# =============================================================================
# 모델 설정
# =============================================================================
CONFIDENCE_THRESHOLD=50
OVERLAP_THRESHOLD=30
MAX_FILE_SIZE_MB=10

# =============================================================================
# 백엔드 API 설정
# =============================================================================
BACKEND_URL=http://localhost:8080
BACKEND_API_KEY=your_backend_api_key

# =============================================================================
# 테스트 설정
# =============================================================================
TEST_MODE=true
ENABLE_MOCK_DATA=true
LOG_LEVEL=INFO
